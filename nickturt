import turtle               # allows us to use the turtles library

window = turtle.Screen()        # creates a graphics window

nick = turtle.Turtle()

nick.speed(0)
nick.pen(pencolor = "blue", pensize = 40)   # create a turtle named nick
for i in range(72):
    nick.forward(250)
    nick.backward(250)
    nick.left(5)
nick.pen(pencolor = "black", pensize = 20)   # create a turtle named nick
for i in range(90):
    nick.forward(225)
    nick.backward(225)
    nick.left(4)
nick.pen(pencolor = "red", pensize = 1)
for i in range(60):
    nick.forward(50)
    nick.right(30)
    nick.forward(50)
    nick.left(50)
    nick.forward(30)
